{
  "name": "JFinalAPP",
  "version": "1.1.2-beta",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "cross-env RCT_METRO_PORT=8081 node node_modules/react-native/local-cli/cli.js run-android",
    "test": "jest",
    "tsc": "tsc",
    "build-android": "cd android && ./gradlew assembleRelease",
    "build-android-clean": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "clean-android": "cd android && ./gradlew clean",
    "upload-android": "pushy uploadApk android/app/build/outputs/apk/release/app-release.apk",
    "publish-android": "pushy bundle --platform android",
    "publish-ios": "pushy bundle --platform ios"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "antd-mobile-rn": "^2.3.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "mobx": "^4",
    "mobx-react": "^5.2.8",
    "qs": "^6.5.2",
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-cheerio": "^1.0.0-rc.4",
    "react-native-elements": "^1.0.0-beta5",
    "react-native-image-crop-picker": "^0.21.2",
    "react-native-material-ui": "^1.30.1",
    "react-native-push-notification": "^3.1.1",
    "react-native-render-html": "^3.10.0",
    "react-native-router-flux": "^4.0.5",
    "react-native-storage": "^0.2.3",
    "react-native-tab-view": "^1.2.0",
    "react-native-update": "^5.1.3",
    "react-native-vector-icons": "^6.0.1",
    "react-native-webview": "^2.5.0",
    "rn-fetch-blob": "^0.10.13",
    "teaset": "^0.5.9"
